/**index.wxss**/
@import "../../components/myModal/index.wxss";
.flexbox { display:-webkit-box; display:-webkit-box-flex; display:box; display:-webkit-flex; display:flex; -webkit-box-pack:justify; -webkit-justify-content:space-around; }
.flexbox > .flex-item { -webkit-box-flex:1; box-flex:1; flex:1;  }
.words_line2 { display: -webkit-box;display: box;-webkit-line-clamp: 2;-webkit-box-orient: vertical;overflow: hidden; }
::-webkit-scrollbar { width: 0; height: 0; color: transparent;}

page{ height:100%; font:26rpx/40rpx "微软雅黑"; }
.container{ text-align:center; background:#f3f3f3; padding:0 40rpx;  box-sizing:border-box;  height:100%; overflow-y:scroll;-webkit-overflow-scrolling:touch; box-sizing:border-box; position:relative; }
.backcolor{ background:#2F2E33; height:90%; width:100%; position:absolute; left:0; top:0; z-index:0; }
.main{ position:relative; z-index:10; }
.container .bala{ font:28rpx/40rpx "微软雅黑"; color:#f3f3f3; padding-top:30rpx; }
.container .note{ font:26rpx/40rpx "微软雅黑"; color:#999; padding:20rpx 0;}
.container .itemList{ text-align:left;}
.container .itemList .con{ display:inline-block; width:50%; text-align:center; padding:25rpx 20rpx; box-sizing:border-box;}
.container .itemList .item{ border-radius:15rpx; background:url('data:image/png;base64,')no-repeat; background-size:100% 100%; }
.container .itemList .item .val{ font-size:35rpx; padding:30rpx 0; color:#FDD01B; border-radius:15rpx 15rpx 0 0; overflow:hidden;}
.container .itemList .item .val .num{ font-size:50rpx; font-weight:bold; display:inline-block;  }
.container .itemList .item .gift{ font-weight:bold; height:60rpx; padding:10rpx 0; position:relative; border-radius:0 0 15rpx 15rpx; }
.container .itemList .item .gift .value{ position:absolute; display:inline-block; left:50%; margin-left:-110rpx; width:220rpx; height:44rpx; line-height:44rpx; bottom:-22rpx; background:#f3f3f3; border-radius:20rpx; }
.container .itemList .item.active{  background:url('data:image/png;base64,')no-repeat; background-size:100% 100%; }
.container .itemList .item.active .val{ color:#000; background:#FDD01B; }
.container .itemList .item.active .value{ background:#EE2C2C; color:#FDD01B; }

.container .bottomCon{ background:#fff; margin-top:20px; border-radius:15rpx 15rpx 0 0; }
.container .bottomCon .con{ padding:0 60rpx; }
.container .bottomCon .title{ position:relative; line-height:110rpx; height:110rpx; font-weight:bold; background:#FDD01B; border-radius:15rpx 15rpx 0 0; font-size:34rpx; }
.container .bottomCon .title:before{ content:''; position:absolute; top:100rpx; left:-5rpx; display:inline-block; width:20rpx; height:20rpx; background:#2F2E33; border-radius:10rpx; }
.container .bottomCon .title:after{ content:''; position:absolute; top:100rpx; right:-5rpx; display:inline-block; width:20rpx; height:20rpx; background:#2F2E33; border-radius:10rpx; }
.container .bottomCon .val{  font-size:45rpx; padding:60rpx 0; color:#FDD01B; display:inline-block; }
.container .bottomCon .val .num { font-size:60rpx; font-weight:bold; display:inline-block;  }
.container .bottomCon .btn{  box-shadow:0 0.1rem 0.2rem rgba(255, 198,0,0.5); font:30rpx/90rpx "微软雅黑"; font-weight:bold; height:90rpx;  background:#FDD01B; border-radius:45rpx; margin-bottom:25rpx; }
.container .bottomCon .tip{ color:#333; padding:10rpx 0; }
.container .bottomCon .instruct{ color:#999; padding:25rpx 0; }