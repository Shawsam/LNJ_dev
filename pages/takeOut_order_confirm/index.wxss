/**index.wxss**/
.flexbox { display:-webkit-box; display:-webkit-box-flex; display:box; display:-webkit-flex; display:flex; -webkit-box-pack:justify; -webkit-justify-content:space-around; }
.flexbox > .flex-item { -webkit-box-flex:1; box-flex:1; flex:1;  }
::-webkit-scrollbar { width: 0; height: 0; color: transparent;}

page{ height:100%; font:26rpx/40rpx "微软雅黑"; }
.backcolor{ background:#2F2E33; height:380rpx; width:100%; position:absolute; left:0; top:0; z-index:0; }
.container{ background:#f3f3f3; box-sizing:border-box;  height:100%; overflow-y:scroll;-webkit-overflow-scrolling:touch;  position:relative; padding:80rpx 0 110rpx 0;}

.topCon{ background:#fff; border-radius:10px; box-shadow:2rpx 2rpx 6rpx rgba(0,0,0,0.1); position:relative; }
.topCon .title{ height:110rpx; font:32rpx/110rpx "微软雅黑"; font-weight:bold; text-align:center; background:#FDD01B; border-radius:20rpx 20rpx 0 0; }
.topCon .title image{ height:50rpx; width:150rpx; display:inline-block; vertical-align:middle; margin-right:15rpx;}
.topCon .info{ padding:35rpx 150rpx 90rpx 35rpx; position:relative;  }
.topCon .info view{ font:28rpx/45rpx "微软雅黑"; }
.topCon .info .edit{ position:absolute; padding:30rpx 25rpx; right:25rpx; top:50rpx; height:30rpx; line-height:30rpx; border-left:1px solid #5A3A01; color:#5A3A01; }
.topCon .tip{ position:absolute; padding-left:15rpx; bottom:-35rpx; left:50%; margin-left:-200rpx;  font:30rpx/70rpx "微软雅黑"; height:70rpx; width:400rpx; text-align:center;  background:#FDD01B; box-shadow:0 10rpx 20rpx rgba(255, 198,0,0.5); border-radius:40px; }
.topCon .tip image{ position:absolute; height:60rpx; width:60rpx; display:inline-block; top:5rpx; left:5rpx; }
.topCon .tip .time{ display:inline-block; font-weight:bold; margin-left:10rpx; }

.main{ padding:0 60rpx; }

.main .list{ margin:115rpx 0 80rpx 0; }
.main .list .total{ padding:20rpx 15rpx; border-bottom:1px solid #e8e8e8; }
.main .list .total view{ display:inline-block; font:28rpx/40rpx "微软雅黑"; vertical-align:top; }
.main .list .total .name{ width:75%; font:30rpx/40rpx "微软雅黑"; }
.main .list .total .price{ width:25%; text-align:right; font-weight:bold; }

.main .list .item{ padding:20rpx 15rpx; }
.main .list .item view{ display:inline-block; font:28rpx/40rpx "微软雅黑"; vertical-align:top; }
.main .list .item .name{ width:55%; font-weight:bold; }
.main .list .item .name view{ font-weight:bold; }
.main .list .item .price{ width:20%; text-align:right; color:#333;}
.main .list .item .num{ width:25%; text-align:center; color:#333;}
.main .list .coupon .price{ color:#E82B63; }


.main .cont{ background:#fff; border-radius:10rpx; padding:0 40rpx; margin-bottom:30rpx; box-shadow:0 0rem 12rpx rgba(0, 0,0,0.3);}
.main .cont .con1 view{ display:inline-block; }
.right{position:absolute;top:50%;right:0; -webkit-transform: translate(0,-50%);-moz-transform:translate(0,-50%);transform: translate(0,-50%);}
.right1{position:absolute;top:0;right:0;width:100%;text-align:right;}
.con1{line-height:120rpx; position:relative;}
.con2{position:relative; line-height:120rpx;}
.conheight{height:120rpx; font:26rpx/120rpx "微软雅黑";}
.sty1 image{ height:60rpx; width:60rpx; vertical-align:middle; margin-right:15rpx; color:#999;}
.sty1{ font:26rpx/120rpx "微软雅黑"; margin-right:60rpx; }
.sty2 image{ height:60rpx; width:60rpx; vertical-align:middle; margin-right:15rpx; color:#999;} 
.sty2{ font:26rpx/120rpx "微软雅黑"; }
.sty1.active{ color:#FC8D17; }
.sty2.active{ color:#FC8D17; }

.arrodow{width:24rpx; height:16rpx; vertical-align:middle; margin-left:20rpx;}
.rote{transform:rotate(-90deg);-ms-transform:rotate(-90deg);-moz-transform:rotate(-90deg);-webkit-transform:rotate(-90deg);-o-transform:rotate(-90deg);position:relative;top:-2px;}
.togg{transform:rotate(-180deg);-ms-transform:rotate(-180deg);-moz-transform:rotate(-180deg);-webkit-transform:rotate(-180deg);-o-transform:rotate(-180deg);position:relative;top:0;}
.tota_bar{color:#ed145b; font:36rpx/90rpx "微软雅黑"; }
.payview{ display:inline-block; }
.tag{ height:50rpx; width:50rpx; vertical-align:middle; margin-right:25rpx; }
.colored{ color:#e5004f; }

.carditem .info1 > view{ color:#ed145b!important; }

.hide_cont{border-top: 1px solid hsl(0, 0%, 93%); padding:24rpx 0; background:#fff; display:none!important;}
.cont.slide .hide_cont{ display:block!important;}
.cont.slide .hide_cont .arrodow{transform:rotate(-180deg);-ms-transform:rotate(-180deg);-moz-transform:rotate(-180deg);-webkit-transform:rotate(-180deg);-o-transform:rotate(-180deg);position:relative;top:0;}
.cont .con1.slide .hide_cont{ display:block!important;}
.cont .con1.slide .hide_cont .arrodow{transform:rotate(-180deg);-ms-transform:rotate(-180deg);-moz-transform:rotate(-180deg);-webkit-transform:rotate(-180deg);-o-transform:rotate(-180deg);position:relative;top:0;}

.hide_cont .tips{ border-bottom:1px solid #eeeeee; margin-top:-13px; line-height:45px; margin-bottom:10px;}
.hide_cont .tel_change{ display:block!important; height:40px;}
.hide_cont .ensure{display: inline-block; width:110rpx; line-height:60rpx; background:#FDD01B; font-weight:bold; float:right; text-align:center; border-radius:6rpx;  }
.hide_cont #telphone{ display: inline-block; width:380rpx; border:1px solid #b5b5b5; line-height:60rpx; height:60rpx; border-radius:6rpx; padding-left:24rpx; vertical-align:top;}
.hide_cont #demand{ width:100%; margin-bottom:20rpx; height:200rpx; padding:8rpx 15rpx; font:24rpx/40rpx "微软雅黑"; border-radius:6rpx; box-sizing:border-box; border:1px solid #e8e8e8;}
.hide_cont .text{ width:100%; margin-bottom:20rpx; height:200rpx; padding:20rpx 25rpx; font:24rpx/40rpx "微软雅黑"; border-radius:6rpx; box-sizing:border-box; border:1px solid #e8e8e8; }

.panel_cover{position:fixed; width:100%; height:100%; background:rgba(0,0,0,0.7); top:0; left:0; z-index:105; }
.panel .tip_panel{ z-index:200; position:fixed; box-sizing:border-box; top:10%; max-height:80%; width:80%; left:10%; background:#fff; border-radius:10rpx; padding-top:90rpx;}
.panel .tip_panel .hd-title{  z-index:200; position:absolute; top:0; width:100%; height:90rpx; text-align:center; background:#FDD01B; font:30rpx/90rpx "微软雅黑"; font-weight:bold; border-radius:10rpx 10rpx 0 0; }
.panel .tip_panel .confirmBtn{ font:30rpx/85rpx "微软雅黑"; text-align:center; font-weight:bold; border-top:1px solid #e8e8e8;  }
.panel .tip_panel .info{ padding:30rpx 0; }
.panel .tip_panel .info view{ font:28rpx/60rpx "微软雅黑"; text-align:center; }

.noscroll{ overflow:hidden; } 

.footer{ z-index:100; position:fixed; bottom:0; height:110rpx; width:100%; background:#fff; box-sizing:border-box; box-shadow:inset 0 1px 0 0 rgba(0,0,0,0.05); overflow:hidden; zoom:1; padding-right:330rpx; }
.footer .userfee{ text-align:right; color:#e5004f; font:30rpx/50rpx "微软雅黑"; font-weight:bold; padding-top:10rpx; vertical-align:bottom;}
.footer .userfee .fee{ font:40rpx/50rpx "微软雅黑"; font-weight:bold; }
.footer .discount{ text-align:right; font:26rpx/30rpx "微软雅黑";  }
.footer .detailBtn{ border-radius:4rpx; border:1px solid #bfbfbf; color:#333; font:24rpx/36rpx "微软雅黑"; padding:0 16rpx; margin-top:40rpx; }
.footer .checkBtn{ position:absolute; top:0; right:0; height:110rpx;  font:36rpx/110rpx "微软雅黑"; width:300rpx; background:#FDD01B; font-weight:bold; text-align:center; }
.footer .checkBtn.num:active{ opacity:0.8;  }




